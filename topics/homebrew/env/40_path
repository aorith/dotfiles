add_to_path "/usr/local/opt/diffutils/bin"
add_to_path "/usr/local/opt/grep/libexec/gnubin"
add_to_path "/usr/local/opt/gnu-sed/libexec/gnubin"
add_to_path "/usr/local/opt/curl/bin"
